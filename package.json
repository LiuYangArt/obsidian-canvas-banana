{
    "name": "canvas-banana",
    "version": "0.10.5",
    "description": "Canvas AI Plugin - use Gemini AI in Canvas view, support text / image / node generation.",
    "main": "main.js",
    "scripts": {
        "version-sync": "node version-bump.mjs",
        "dev": "npm run version-sync && node esbuild.config.mjs",
        "build": "npm run version-sync && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "lint": "eslint .",
        "version": "npm run version-sync && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "canvas",
        "ai",
        "gemini"
    ],
    "author": "LiuYang",
    "license": "GPL-3.0",
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "eslint": "^9.39.2",
        "eslint-plugin-obsidianmd": "^0.1.9",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "^5.9.3"
    }
}
